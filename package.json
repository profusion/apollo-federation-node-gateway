    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^16.11.0",
    "husky": "^8.0.1",
    "install-peers-cli": "^2.2.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "newman": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.2.3"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
